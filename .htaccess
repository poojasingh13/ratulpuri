RewriteEngine On

RewriteRule ^index\.php$ http://www.ratulpuri.com/ [L,R=301]

RewriteRule ^article[0-9]+$ http://www.ratulpuri.com/ [L,R=301]

RewriteRule ^ratulpuripower\.php$ http://www.ratulpuri.com/ [L,R=301]
RewriteRule ^ratulpuriagenda\.php$ http://www.ratulpuri.com/ [L,R=301]
RewriteRule ^ratulpurienergy\.php$ http://www.ratulpuri.com/ [L,R=301]

RewriteCond %{HTTP_HOST} ^www.ratulpuri.com/index.php#$ [NC]
RewriteRule ^(.*)$ http://www.ratulpuri.com/$1 [L,R=301]



RewriteCond %{HTTP_HOST} !^www\.
RewriteRule ^(.*)$ http://www.%{HTTP_HOST}/$1 [R=301,L]

